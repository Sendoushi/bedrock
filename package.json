{
  "name": "bedrock",
  "version": "0.3.3",
  "description": "Frontend top layer",
  "authors": [
    "Joel Santos <joel@sendoushi.com>"
  ],
  "main": "./src/",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:sendoushi/bedrock.git"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-remove-code": "0.0.6",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^6.2.0",
    "glob": "^7.1.1",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "deep-diff": "^0.3.4",
    "lodash": "^4.17.2",
    "page": "^1.7.1",
    "redux": "^3.6.0"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "eslint": "eslint ./src/**/*.js",
    "mocha": "BABEL_ENV=test mocha ./test/test.js --require ./test/babel.js",
    "test": "npm run eslint && npm run mocha",
    "prebuild": "npm run clean && npm run test || true",
    "build": "BABEL_ENV=production babel src --out-dir dist --source-maps inline --ignore *.spec.js,_test/**/*",
    "prepublish": "npm run test"
  }
}
